{
	//I believe eslint defaults to EcmaScript5. You'll want to override to 6.
	"parserOptions": {
   		"ecmaVersion": 6
 	},
 	"rules":{
 		"indent": [2 , "tab"],
 		"no-tabs": 0,
 		"quotes": [2 , "single"],
 		"linebreak-style": [2 , "unix"],
 		"semi": [2 , "always"]
 	},
    "extends": ["eslint:recommended", "google"]
}